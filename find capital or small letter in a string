#program name-find capital or small letter in a string

str=input("enter the string")
str2=" "
str3=" "
for i in range (0,len(str)):
	if(str[i].isupper()):
		str2=str2+str[i]
	else:
		str3=str3+str[i]
print("capital letter string" , str2)
print("small letter string" ,str3)
