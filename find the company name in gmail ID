#program name-find the company name in gmail ID.
#ex- input-vishaljain@google.com
#    output-google

e=input("enter your gmail")
for x in range(len(e)):
    if ord(e[x])==ord('@'):
        while(ord (e[x+1])!=ord('.')):
              print(e[x+1],sep='',end='')
              x+=1
